files:
  "/etc/nginx/conf.d/add_dev_port.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
        upstream nodejswebpack {
            server 127.0.0.1:8082;
            keepalive 256;
        }
        server {
            listen 8082;
            location / {
                proxy_pass  http://nodejswebpack;
            }
        }
