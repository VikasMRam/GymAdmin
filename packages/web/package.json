{
  "name": "@sly-react/web",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server",
  "sideEffects": false,
  "scripts": {
    "preinstall": "../../scripts/check-install-allowed.sh",
    "start": "node .",
    "start:prod": "npm run env:prod -- node .",
    "start:staging": "npm run env:staging -- node .",
    "predev": "npm run prebuild",
    "dev": "npm run build:clean && babel-node -i ../../**/node_modules --config-file ../../babel.config.js src/server.js",
    "dev:https": "HOST=https://www.lvh.me API_URL=https://www.lvh.me/v0 NODE_TLS_REJECT_UNAUTHORIZED=0 npm run dev -- --https",
    "prestaging": "npm run prebuild",
    "preprod": "npm run prebuild",
    "staging": "npm run build:staging && npm run start:staging",
    "production": "npm run build:prod && npm run start:prod",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles \"public/**/*\" dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "webpack",
    "build:prod": "npm run env:prod -- npm run build",
    "build:staging": "npm run env:staging -- npm run build",
    "env:dev": "../../node_modules/.bin/cross-env NODE_ENV=development SLY_ENV=development",
    "env:prod": "../../node_modules/.bin/cross-env NODE_ENV=production SLY_ENV=production",
    "env:staging": "../../node_modules/.bin/cross-env NODE_ENV=production SLY_ENV=staging",
    "env:test": "../../node_modules/.bin/cross-env NODE_ENV=test SLY_ENV=test",
    "test": "../../node_modules/.bin/jest --passWithNoTests",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "../../node_modules/.bin/eslint src/*.js src/external src/controllers src/components/pages/wizards src/containers/wizards src/components/common src/components/helpers src/components/atoms src/components/molecules src/components/wizards src/constants private webpack.config.js"
  },
  "devDependencies": {
    "@babel/node": "7.12.1",
    "@loadable/babel-plugin": "5.12.0",
    "@loadable/webpack-plugin": "5.12.0",
    "babel-loader": "8.1.0",
    "cssmin": "0.4.3",
    "eslint-import-resolver-webpack": "0.11.1",
    "estree-walk": "2.2.0",
    "history": "4.10.1",
    "jest-websocket-mock": "2.0.2",
    "mock-socket": "9.0.3",
    "raw-loader": "4.0.0",
    "react-hot-loader": "4.12.20",
    "redbox-react": "1.6.0",
    "redux-logger": "3.0.6",
    "uglify-es": "3.3.9",
    "url-loader": "4.0.0",
    "webpack": "4.42.1",
    "webpack-blocks": "2.1.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge-and-include-globally": "2.1.24",
    "webpack-node-externals": "1.7.2",
    "webpack-spawn-plugin": "0.3.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@loadable/component": "5.13.1",
    "@loadable/server": "5.13.1",
    "@tinymce/tinymce-react": "3.5.0",
    "body-scroll-lock": "^3.1.5",
    "copyfiles": "2.3.0",
    "dayjs": "1.8.23",
    "emotion": "10.0.27",
    "emotion-server": "10.0.27",
    "express": "4.17.1",
    "google-map-react": "2.1.8",
    "ifvisible.js": "1.0.6",
    "intersection-observer": "0.7.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.19",
    "memoize-one": "5.1.1",
    "object-path-immutable": "4.1.0",
    "query-string": "6.11.1",
    "react-cookie": "4.0.3",
    "react-datepicker": "2.14.1",
    "react-dom": "16.14.0",
    "react-ga": "2.7.0",
    "react-google-maps": "9.4.5",
    "react-helmet": "6.1.0",
    "react-lazy": "1.1.0",
    "react-measure": "2.3.0",
    "react-modal": "3.11.2",
    "react-router-dom": "5.2.0",
    "react-s3-uploader": "4.9.2",
    "react-select": "3.1.0",
    "react-sortable-hoc": "1.11.0",
    "react-swipeable-views": "0.13.9",
    "react-tooltip": "4.1.2",
    "react-transition-group": "4.3.0",
    "react-use-dimensions": "^1.2.1",
    "redux-object": "0.5.10",
    "redux-saga": "1.1.3",
    "redux-saga-thunk": "0.7.3",
    "redux-thunk": "2.3.0",
    "rimraf": "3.0.2",
    "serialize-error": "6.0.0",
    "serialize-javascript": "3.1.0",
    "smoothscroll-polyfill": "0.4.4",
    "universal-cookie": "4.0.3",
    "use-resize-observer": "6.1.0",
    "uuid": "7.0.2",
    "validator": "13.0.0",
    "yall-js": "3.1.7"
  }
}
