{
  "name": "sly-react",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server",
  "sideEffects": false,
  "scripts": {
    "start": "node .",
    "start:prod": "npm run env:prod -- node .",
    "start:staging": "npm run env:staging -- node .",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "dev:eb": "npm-run-all -p dev storybook",
    "prestaging": "npm run prebuild",
    "preprod": "npm run prebuild",
    "staging": "npm run build:staging && npm run start:staging",
    "production": "npm run build:prod && npm run start:prod",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles \"public/**/*\" dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "webpack",
    "build:prod": "npm run env:prod -- npm run build",
    "build:staging": "npm run env:staging -- npm run build",
    "env:dev": "cross-env NODE_ENV=development SLY_ENV=development",
    "env:prod": "cross-env NODE_ENV=production SLY_ENV=production",
    "env:staging": "cross-env NODE_ENV=production SLY_ENV=staging",
    "env:test": "cross-env NODE_ENV=test SLY_ENV=test",
    "test": "npm run env:test -- jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src/external src/components/common src/components/helpers src/components/atoms src/components/molecules src/constants src/propTypes private webpack.config.js",
    "format": "prettier-eslint --write",
    "format-all": "npm run format -- \"src/**/*.js\"",
    "storybook": "start-storybook -s ./public -p 9001 -c private/storybook"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^sly[/](components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^sly[/](.*)$": "<rootDir>/src/$1",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/shim.js",
      "<rootDir>/private/jest/setupTests.js"
    ],
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-options": "5.0.11",
    "@storybook/addons": "5.0.11",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^5.16",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^24.8.0",
    "jest-babel": "^1.0.1",
    "jest-junit": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^3.1.0",
    "prettier-eslint-cli": "^4.7.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "webpack": "^4.29.6",
    "webpack-blocks": "^2.0.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge-and-include-globally": "^2.1.16",
    "webpack-node-externals": "^1.7.2",
    "webpack-sort-chunks": "^0.2.0",
    "webpack-spawn-plugin": "^0.3.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "color": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.1.3",
    "cssmin": "^0.4.3",
    "dayjs": "^1.8.11",
    "delay": "^2.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.6",
    "history": "^4.6.1",
    "immer": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-api-normalizer": "^0.4.7",
    "lodash": "^4.17.11",
    "normalizr": "3.2.3",
    "object-path-immutable": "^3.0.2",
    "prop-types": "^15.5.8",
    "query-string": "^5.0.0",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-cookie": "^1.0.5",
    "react-dom": "^16.3.2",
    "react-dotdotdot": "^1.2.3",
    "react-ga": "^2.5.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.1",
    "react-lazy": "^0.5.1",
    "react-measure": "^2.0.2",
    "react-modal": "^3.4.4",
    "react-number-format": "^3.3.4",
    "react-places-autocomplete": "^7.1.1",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.1.1",
    "react-router-server": "^4.2.1",
    "react-stickynode": "^1.4.1",
    "react-swipeable-views": "^0.12.13",
    "react-tooltip": "^3.6.1",
    "react-transition-group": "^2.5.0",
    "redbox-react": "^1.6.0",
    "redux": "^3.6.0",
    "redux-form": "^7.2.1",
    "redux-logger": "^3.0.6",
    "redux-object": "0.5.5",
    "redux-saga": "^0.16.0",
    "redux-saga-thunk": "^0.5.3",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.1",
    "serialize-error": "^2.1.0",
    "serialize-javascript": "^1.5.0",
    "sinon": "^4.4.6",
    "smoothscroll-polyfill": "^0.4.3",
    "styled-components": "^2.4.0",
    "styled-theme": "^0.3.0",
    "styled-tools": "^1.7.1",
    "uglify-es": "^3.3.9",
    "universal-cookie": "^3.0.4",
    "url-loader": "^0.6.2",
    "uuid": "^3.2.1",
    "validator": "^9.0.0"
  }
}
