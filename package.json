{
  "name": "SlyReact",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server",
  "scripts": {
    "start": "node .",
    "start:prod": "npm run env:prod -- node .",
    "start:staging": "npm run env:staging -- node .",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "dev:eb": "npm-run-all -p dev storybook",
    "prestaging": "npm run prebuild",
    "preprod": "npm run prebuild",
    "staging": "npm run build:staging && npm run start:staging",
    "production": "npm run build:prod && npm run start:prod",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles \"public/**/*\" dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "webpack",
    "build:prod": "npm run env:prod -- npm run build",
    "build:staging": "npm run env:staging -- npm run build",
    "env:dev": "cross-env NODE_ENV=development SLY_ENV=development",
    "env:prod": "cross-env NODE_ENV=production SLY_ENV=production",
    "env:staging": "cross-env NODE_ENV=production SLY_ENV=staging",
    "env:test": "cross-env NODE_ENV=test SLY_ENV=test",
    "test": "npm run env:test -- jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src private webpack.config.js",
    "format": "prettier-eslint --write",
    "format-all": "npm run format -- \"src/**/*.js\"",
    "storybook": "start-storybook -s ./public -p 9001 -c private/storybook"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^sly[/](components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^sly[/](.*)$": "<rootDir>/src/$1",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/shim.js",
      "<rootDir>/private/jest/setupTests.js"
    ],
    "verbose": true
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.0-rc.2",
    "@storybook/addon-options": "3.4.0-rc.2",
    "@storybook/addons": "3.4.0-rc.2",
    "@storybook/react": "3.4.0-rc.2",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^22.4.3",
    "jest-cli": "^22.0.1",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1"
  },
  "dependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "color": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.1.3",
    "cssmin": "^0.4.3",
    "delay": "^2.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.6",
    "history": "^4.6.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-junit": "^3.7.0",
    "json-api-normalizer": "^0.4.7",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "normalizr": "3.2.3",
    "prop-types": "^15.5.8",
    "query-string": "^5.0.0",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-cookie": "^1.0.5",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.3.2",
    "react-dotdotdot": "^1.2.3",
    "react-ga": "^2.5.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.1.1",
    "react-lazy": "^0.5.1",
    "react-measure": "^2.0.2",
    "react-modal": "^3.4.4",
    "react-number-format": "^3.3.4",
    "react-places-autocomplete": "^7.1.1",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-server": "^4.2.1",
    "react-stickynode": "^1.4.1",
    "react-swipeable-views": "^0.12.13",
    "react-tooltip": "^3.6.1",
    "react-transition-group": "^2.5.0",
    "redbox-react": "^1.6.0",
    "redux": "^3.6.0",
    "redux-form": "^7.2.1",
    "redux-logger": "^3.0.6",
    "redux-object": "0.5.5",
    "redux-saga": "^0.16.0",
    "redux-saga-thunk": "^0.5.3",
    "rimraf": "^2.6.1",
    "serialize-error": "^2.1.0",
    "serialize-javascript": "^1.5.0",
    "simple-load-script": "^1.0.2",
    "sinon": "^4.4.6",
    "smoothscroll-polyfill": "^0.4.3",
    "styled-components": "^2.4.0",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.4.0",
    "uglify-es": "^3.3.9",
    "universal-cookie": "^3.0.4",
    "url-loader": "^0.6.2",
    "uuid": "^3.2.1",
    "validator": "^9.0.0",
    "webpack": "^2.6.0",
    "webpack-assets-by-type-plugin": "^0.1.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-server-source-map": "^0.2.0",
    "webpack-blocks-split-vendor": "^0.2.1",
    "webpack-child-config-plugin": "^0.1.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-merge-and-include-globally": "^2.0.11",
    "webpack-node-externals": "^1.5.4",
    "webpack-spawn-plugin": "^0.1.1"
  }
}
