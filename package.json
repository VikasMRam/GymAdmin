{
  "name": "sly-react",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server",
  "sideEffects": false,
  "scripts": {
    "start": "node .",
    "start:prod": "npm run env:prod -- node .",
    "start:staging": "npm run env:staging -- node .",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "dev:eb": "npm-run-all -p dev storybook",
    "prestaging": "npm run prebuild",
    "preprod": "npm run prebuild",
    "staging": "npm run build:staging && npm run start:staging",
    "production": "npm run build:prod && npm run start:prod",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles \"public/**/*\" dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "webpack",
    "build:prod": "npm run env:prod -- npm run build",
    "build:staging": "npm run env:staging -- npm run build",
    "env:dev": "cross-env NODE_ENV=development SLY_ENV=development",
    "env:prod": "cross-env NODE_ENV=production SLY_ENV=production",
    "env:staging": "cross-env NODE_ENV=production SLY_ENV=staging",
    "env:test": "cross-env NODE_ENV=test SLY_ENV=test",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src/*.js src/external src/controllers src/components/common src/components/helpers src/components/atoms src/components/molecules src/constants src/propTypes private webpack.config.js e2e",
    "format": "prettier-eslint --print-width 120 --write",
    "format-all": "npm run format -- \"src/**/*.js\"",
    "storybook": "start-storybook -s ./public -p 9001 -c private/storybook"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^!raw-loader": "<rootDir>/private/jest/fileMock.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^sly[/](components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^sly[/](.*)$": "<rootDir>/src/$1",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/shim.js",
      "<rootDir>/private/jest/setupTests.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "verbose": true
  },
  "devDependencies": {},
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@emotion/core": "^10.0.22",
    "@loadable/babel-plugin": "^5.11.0",
    "@loadable/component": "^5.11.0",
    "@loadable/server": "^5.11.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-options": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
