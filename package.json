{
  "name": "sly-react",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=12.16.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "lerna clean --yes",
    "create:package": "./scripts/new-package.sh",
    "bootstrap": "npm run clean && IS_SLY_SETUP=true lerna bootstrap --hoist",
    "postinstall": "npm run bootstrap $PI_OPTS",
    "lint": "lerna run --stream lint",
    "test": "lerna run --stream test",
    "format": "prettier-eslint --print-width 120 --ignore **/node_modules/** \"$PWD/packages/*/src/**/*.js\"",
    "production": "npm --prefix=packages/web run ${SLY_ENV:-dev}",
    "build:web:prod": "npm --prefix=packages/web run build:prod",
    "start:web": "npm --prefix=packages/web run ${SLY_ENV:-dev}",
    "start:web:prod": "npm --prefix=packages/web run start:prod",
    "start:mobile": "npm --prefix=packages/mobile run ${PLATFORM:-ios}",
    "start:storybook": "npm --prefix=packages/storybook start",
    "start:storybook:mobile": "npm --prefix=packages/storybook run start:mobile:${PLATFORM:-ios}"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "6.7",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "prettier-eslint-cli": "5.0.0",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-do-expressions": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-proposal-pipeline-operator": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "core-js": "3.6.5",
    "@babel/preset-react": "7.9.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-styled-components": "1.10.7",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-jest": "26.1.0",
    "redux-mock-store": "1.5.4",
    "jest": "26.1.0",
    "jest-junit": "11.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "lerna": "3.22.1"
  },
  "dependencies": {
    "cross-env": "7.0.2",
    "hoist-non-react-statics": "3.3.2",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
    "redux": "4.0.5",
    "redux-form": "8.3.6",
    "styled-components": "5.1.1",
    "styled-theme": "0.3.3",
    "styled-tools": "1.7.1"
  }
}
