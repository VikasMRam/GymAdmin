{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-1"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        ["styled-components", {
          "minify": false
        }]
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        ["styled-components", {
          "fileName": false,
          "displayName": true,
          "minify": false
        }]
      ]
    },
    "staging": {
      "plugins": [
        "transform-react-remove-prop-types",
        ["styled-components", {
          "displayName": true
        }]
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        ["styled-components", {
          "displayName": true
        }]
      ]
    }
  }
}
